<h1>Keeper# schools rank</h1>
      <ul>
      <% @schools_final_list.each do |school| %>
        <br>
        <li>nome do google: <%= school["name"] %></li>
        <li>nome do dados: <%= (school["atividades_complementares"].empty?) ? " " : school["atividades_complementares"].first["nome"] %></li>
        <li>distancia: <%= school["geometry"]["location"]["distance"] %> Km</li>
        <li>latitude: <%= school["geometry"]["location"]["lat"] %></li>
        <li>longitude: <%= school["geometry"]["location"]["lng"] %></li>
        <li>endere√ßo: <%= school["vicinity"]%></li>
        <li>telefone: <%= (school["atividades_complementares"].empty?) ? "sem dados" : school["atividades_complementares"].first["telefone"]%></li>
        <li>email: <%= (school["atividades_complementares"].empty?) ? "sem dados" : school["atividades_complementares"].first["email"]%></li>
        <li>atividade ofertada:</li>
        <br>
        <% unless school["atividades_complementares"].empty? %>
            <% school["atividades_complementares"].each do |atividade| %>
            <li><%= atividade["nome_atividade"]%></li>
            <% end %>
        <% end %>
        <br>
        <br>
        <br>
      <% end %>
      </ul>